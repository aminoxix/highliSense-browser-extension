{
  "name": "highliSense",
  "description": "highly sensible - helps YOU understands the context",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["activeTab", "tabs", "scripting", "storage"]
}
